{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",       
      "use": "@vercel/node"        
    },
    {
      "src": "videopage.html",      
      "use": "@vercel/static"      
    }
  ],
  "routes": [
 
    {
      "src": "/api/(.*)",          
      "dest": "/api/index.js"      
    },
  
    {
      "src": "/",                   
      "dest": "/videopage.html"    
    }
  ]
}

